[
  {
    "group": "admin",
    "permissions": [
      {
        "resource": "*",
        "methods": "*",
        "action": "allow"
      }
    ]
  },
  {
    "group": "user",
    "permissions": [
      {
        "resource": [ "/User/Get/*",
          "/Targets/Get/*", "/Targets/GetAll*",
          "/TargetDefinitions/Get/*", "/TargetDefinitions/GetAll*",
          "/Datasets/Get/*", "/Datasets/GetAll*",
          "/DatasetItems/Get/*", "/DatasetItems/GetAll/*"
        ],
        "methods": ["GET"],
        "action": "allow"
      },
      {
        "resource": ["/User/Update/*" ],
        "methods": ["PUT"],
        "action": "allow"
      },
      {
        "resource": ["/Answers/SubmitBatchAnswer"],
        "methods": ["POST"],
        "action": "allow"
      }
    ]
  },
  {
    "group": "guest",
    "permissions": [
      {
        "resources": ["/", "/auth", "/SSOCallback"],
        "methods": [ "GET"],
        "action": "allow"
      }
    ]
  }
]
